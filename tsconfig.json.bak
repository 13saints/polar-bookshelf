{
    "compilerOptions": {

        "noImplicitAny": false,
        "strictNullChecks": false,
        "noImplicitThis": false,


        "sourceMap": true,
        "target": "ES5",
        "module": "commonjs",
        "emitDecoratorMetadata": false,
        "experimentalDecorators": false,
        "outDir": "web",
        "allowJs": false,
        "stripInternal": true,
        "checkJs": false,

        "noUnusedLocals": true,
        "noUnusedParameters": true,

        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*"]
        },
        "lib": ["es2015", "dom"]

    },
    "include": [
        "web/**/*"
    ],
    "exclude": [

    ],
    "types": [
        "mocha",
        "electron"
    ],
    "typeRoots": [
        "./node_modules/@types"
    ]
}
